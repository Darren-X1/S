[Script]
# > JD签到(api.m.jd.com)
JD_DailyBonus = type=cron,cronexp="1 0,23 * * *",script-path=https://raw.githubusercontent.com/NobyDa/Script/master/JD-DailyBonus/JD_DailyBonus.js

Tasks: JD618 = type=cron,cronexp="10,20,30,40,50 0,09 * * *",script-path=https://raw.githubusercontent.com/chavyleung/scripts/master/jd/jd.618.adapt.js,wake-system=true,timeout=600
Tasks: JD618.Boom = type=cron,cronexp="0 10,12,18,20 * * *",script-path=https://raw.githubusercontent.com/chavyleung/scripts/master/jd/jd.618.boom.js,wake-system=true
# > tieba签到(tieba.baidu.com)
