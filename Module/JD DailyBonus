#!name=JD DailyBonus
#!desc= Safari浏览器打开https://bean.m.jd.com ,登录点击签到并且出现签到日历后, 如果通知获得cookie成功, 则可以使用此签到脚本。
#!system=ios

[Script]
JD_DailyBonus = type=http-request,pattern=https:\/\/api\.m\.jd\.com\/client\.action.*functionId=signBean,script-path=https://raw.githubusercontent.com/NobyDa/Script/master/JD-DailyBonus/JD_DailyBonus.js
JD_DailyBonus = type=cron,cronexp="3 0 * * *",script-path=https://raw.githubusercontent.com/NobyDa/Script/master/JD-DailyBonus/JD_DailyBonus.js

[MITM]
hostname = %APPEND% api.m.jd.com
